<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <gradient android:startColor="#33000000" android:endColor="#33000000" android:angle="270.0" android:centerColor="#33000000" />
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <gradient android:startColor="#994bad89" android:endColor="#994bad89" android:angle="270.0" android:centerY="1.0" android:centerColor="#994bad89" />
            </shape>
        </clip>
    </item>
    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <gradient android:startColor="#ff4bad89" android:endColor="#ff4bad89" android:angle="270.0" android:centerY="1.0" android:centerColor="#ff4bad89" />
            </shape>
        </clip>
    </item>
</layer-list>